# Base URL that FastAPI uses to call Gitea (inside docker network)
GITEA_URL=http://gitea:3000
# Public URL for clone URLs (what users see)
GITEA_PUBLIC_URL=http://localhost:3000
# Admin personal access token from a Gitea admin account (Settings -> Applications)
GITEA_ADMIN_TOKEN=your-admin-token-here

# Gitea SSH access for CLI commands (for creating user tokens)
GITEA_SSH_HOST=git@localhost
GITEA_SSH_PORT=2222

# Project URL and keys from Supabase settings (API)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_PUB_KEY=your-publishable-key
SUPABASE_SERVICE_KEY=your-service-role-key
